{
  "product": "Rosterserver",
  "name": "@classlink/movie-watchlist-sdk",
  "version": "0.0.0",
  "description": "test project for cls4 workshop",
  "contributors": [],
  "stakeholders": [],
  "main": "dist",
  "scripts": {
    "build": "grunt make:build",
    "check-all": "npm run check-dependencies && npm run check-outdated && npm run check-cl-dependencies && npm test",
    "check-cl-dependencies": "grunt checkClassLinkDependencyVersions",
    "check-dependencies": "depcheck",
    "check-outdated": "check-outdated --ignore-packages \"depcheck,@types/node,redis,p-retry,p-timeout\" --ignore-pre-releases",
    "dev": "mocha -w",
    "devbox": "grunt watch:forPM2",
    "devbox-raw": "grunt watch:forPM2",
    "eslint": "eslint src/*",
    "npmlogin": "aws codeartifact login --tool npm --namespace @classlink --repository npm --domain internal --domain-owner 751250156163",
    "open-codecommit": "start https://us-east-1.console.aws.amazon.com/codesuite/codecommit/repositories/movie-watchlist-sdk/pull-requests?region=us-east-1",
    "open-codepipeline-beta": "start https://us-east-1.console.aws.amazon.com/codesuite/codepipeline/pipelines/release-movie-watchlist-sdk-beta/view?region=us-east-1",
    "open-codepipeline-prod": "start https://us-east-1.console.aws.amazon.com/codesuite/codepipeline/pipelines/release-movie-watchlist-sdk-prod/view?region=us-east-1",
    "test": "mocha",
    "test-beta": "NODE_ENV=beta mocha",
    "test-beta-windows": "set NODE_ENV=beta&&mocha",
    "test-prod": "NODE_ENV=prod mocha",
    "test-prod-windows": "set NODE_ENV=prod&&mocha",
    "test-staging": "NODE_ENV=staging mocha",
    "test-staging-windows": "set NODE_ENV=staging&&mocha"
  },
  "author": "dev.internal@classlink.com",
  "license": "ISC",
  "dependencies": {
    "@classlink/cache": "0.9.0-beta.4",
    "@classlink/database": "0.6.0-beta.5",
    "@classlink/logger": "0.2.0-beta.4",
    "@classlink/secrets-manager-sdk": "0.1.0-beta.21",
    "@hapi/boom": "^9.1.4",
    "base62": "^2.0.1",
    "git": "^0.1.5",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "needle": "^3.1.0",
    "underscore": "^1.13.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.1",
    "@types/needle": "^2.5.3",
    "@types/node": "^12.20.11",
    "@types/sinon-chai": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.27.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "check-outdated": "^2.10.2",
    "cz-conventional-changelog": "^3.0.2",
    "depcheck": "^1.4.3",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "grunt": "1.5.3",
    "grunt-cli": "^1.4.3",
    "grunt-contrib-clean": "^2.0.1",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-ts": "^6.0.0-beta.22",
    "mocha": "^9.2.2",
    "sinon": "^13.0.2",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "initByClara": "0.6.1"
}
